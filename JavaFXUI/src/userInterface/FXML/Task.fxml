<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<ScrollPane maxHeight="1000.0" maxWidth="1000.0" minHeight="400.0" minWidth="400.0" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TaskController">
   <content>
      <BorderPane fx:id="taskBorderPane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="720.0" prefWidth="1000.0">
         <top>
            <ToolBar fx:id="toolBar">
               <items>
                  <HBox fx:id="toolBarHBox">
                     <children>
                      <Button fx:id="runButton" disable="true" mnemonicParsing="false" onAction="#runPressed" text="Run">
                           <HBox.margin>
                              <Insets left="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="PauseButton" disable="true" mnemonicParsing="false" onAction="#pausePressed" text="Pause">
                           <HBox.margin>
                              <Insets left="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="stopButton" disable="true" mnemonicParsing="false" onAction="#stopPressed" text="Stop">
                           <HBox.margin>
                              <Insets left="5.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </items>
            </ToolBar>
         </top>
         <left>
             <Pane fx:id="leftPane" prefHeight="1661.0" prefWidth="300.0" BorderPane.alignment="CENTER">
                 <BorderPane.margin>
                     <Insets />
                 </BorderPane.margin>
                 <ComboBox fx:id="taskSelection" layoutX="28.0" layoutY="52.0" onAction="#taskSelectionPressed" prefWidth="150.0" promptText="-Task Selection-" />
                 <ComboBox fx:id="targetSelection" disable="true" layoutX="28.0" layoutY="103.0" onAction="#targetSelectionPressed" prefWidth="150.0" promptText="-Target-" />
                 <ComboBox fx:id="affectedTargets" disable="true" layoutX="28.0" layoutY="158.0" onAction="#affectedTargetsPressed" prefWidth="150.0" promptText="-Affected Targets-" />
                 <ListView fx:id="currentSelectedTargetListView" disable="true" layoutX="28.0" layoutY="243.0" prefHeight="156.0" prefWidth="190.0" />
                 <Label fx:id="currentSelectedTargetLabel" disable="true" layoutX="61.0" layoutY="217.0" text="Current Selected Targets" />
                 <Label fx:id="processingTimeLabel" disable="true" layoutX="18.0" layoutY="470.0" text="Processing Time (m/s) : " />
                 <Label fx:id="limitedPermanentLabel" disable="true" layoutX="20.0" layoutY="508.0" text="Limited/Permanent : " />
                 <Label fx:id="successRateLabel" disable="true" layoutX="18.0" layoutY="540.0" text="Success Rate :" />
                 <Label fx:id="successRateWithWarnings" disable="true" layoutX="17.0" layoutY="569.0" text="Success Rate&#10;With Warnings : " />
                 <HBox layoutX="131.0" layoutY="508.0">
                     <fx:define>
                         <ToggleGroup fx:id="limitedOrPermanent" />
                     </fx:define>
                     <children>
                         <RadioButton fx:id="limitedRadioButton" disable="true" mnemonicParsing="false" onAction="#limitedOptionPressed" prefWidth="87.0" selected="true" text="Limited" />
                         <RadioButton fx:id="permanentRadioButton" disable="true" mnemonicParsing="false" onAction="#permanentOptionPressed" prefWidth="87.0" text="Permanent">
                        <toggleGroup>
                           <ToggleGroup fx:id="limitedOrPermanent" />
                        </toggleGroup></RadioButton>
                     </children>
                 </HBox>
                  <Slider fx:id="successRateSlider" disable="true" layoutX="103.0" layoutY="542.0" max="1.0" showTickLabels="true" showTickMarks="true" />
                 <Slider fx:id="successRateWithWarningsSlider" disable="true" layoutX="103.0" layoutY="589.0" max="1.0" showTickLabels="true" showTickMarks="true" />
                 <TextField fx:id="processingTimeTextField" disable="true" layoutX="149.0" layoutY="466.0" onAction="#getProcessingTime" promptText="Time In M/S" />
               <TextField fx:id="successWithWarningRateText" disable="true" layoutX="243.0" layoutY="584.0" prefHeight="25.0" prefWidth="45.0" />
               <TextField fx:id="successRateText" disable="true" layoutX="243.0" layoutY="536.0" prefHeight="25.0" prefWidth="45.0" />
               <Button fx:id="ApplyParametersButton" disable="true" layoutX="89.0" layoutY="624.0" mnemonicParsing="false" onAction="#ApplyParametersToTask" text="Apply Parameters" />
               <HBox alignment="CENTER" layoutX="26.0" layoutY="408.0" prefHeight="40.0" prefWidth="207.0">
                  <children>
                     <RadioButton fx:id="fromScratchRadioButton" disable="true" mnemonicParsing="false" onAction="#fromScratchOptionPressed" selected="true" text="From Scratch">
                        <toggleGroup>
                           <ToggleGroup fx:id="scratchOrIncremental" />
                        </toggleGroup>
                     </RadioButton>
                     <RadioButton fx:id="incrementalRadioButton" disable="true" mnemonicParsing="false" onAction="#incrementalOptionPressed" text="Incremental" toggleGroup="$scratchOrIncremental">
                        <HBox.margin>
                           <Insets left="15.0" />
                        </HBox.margin>
                     </RadioButton>
                  </children>
               </HBox>
               <Button fx:id="selectAllButton" disable="true" layoutX="223.0" layoutY="309.0" mnemonicParsing="false" onAction="#selectAllPressed" text="Select All" />
               <Button fx:id="deselectAllButton" disable="true" layoutX="223.0" layoutY="309.0" mnemonicParsing="false" onAction="#deselectAllPressed" text="Deselect All" visible="false" />
             </Pane>
         </left>
         <center>
            <TabPane fx:id="tabPane" BorderPane.alignment="CENTER">
               <tabs>
                  <Tab fx:id="tableViewTabPane" closable="false" onSelectionChanged="#tableViewTabPressed" text="Table View">
                     <content>
                        <Pane prefHeight="656.0" prefWidth="669.0">
                           <children>
                              <TableView fx:id="taskTargetDetailsTableView" layoutX="43.0" layoutY="47.0" prefHeight="220.0" prefWidth="470.0">
                                <columns>
                                  <TableColumn fx:id="numberColumn" prefWidth="22.0" text="#" />
                                  <TableColumn fx:id="targetNameColumn" prefWidth="85.0" text="Target Name" />
                                    <TableColumn fx:id="positionColumn" prefWidth="63.5" text="Position" />
                                    <TableColumn fx:id="directDependsOnColumn" prefWidth="118.0" text="Current Runtime Status" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                              <TextArea fx:id="taskDetailsOnTargetTextArea" disable="true" layoutX="43.0" layoutY="427.0" prefHeight="200.0" prefWidth="432.0" style="-fx-background-color: black;" />
                           </children>
                        </Pane>
                     </content>
                  </Tab>
                  <Tab fx:id="graphViewTabPane" closable="false" onSelectionChanged="#graphViewTabPressed" text="Graph View">
                     <content>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <ScrollPane layoutX="124.0" layoutY="25.0">
                                 <content>
                                    <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                                       <children>
                                          <ImageView fx:id="graphImage" fitHeight="467.0" fitWidth="579.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS" />
                                       </children>
                                    </HBox>
                                 </content>
                              </ScrollPane>
                           </children></Pane>
                     </content>
                  </Tab>
                  <Tab fx:id="logPane" closable="false" onSelectionChanged="#logViewTabPressed" text="Log">
                     <content>
                        <Pane fx:id="zeroSuccessRate" prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <ScrollPane layoutX="14.0" layoutY="14.0">
                                 <content>
                                    <TextArea fx:id="logTextArea" disable="true" editable="false" prefHeight="456.0" prefWidth="535.0" />
                                 </content>
                              </ScrollPane>
                           </children>
                        </Pane>
                     </content>
                  </Tab>
               </tabs>
            </TabPane>
         </center>
      </BorderPane>
   </content>
</ScrollPane>
